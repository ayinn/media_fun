<p id="notice"><%= notice %></p>

<h1>Listing Media</h1>
<div class="table-responsive">
<table class="table table-striped table-condensed">
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Type</th>
      <th>Market</th>
      <th>Target</th>
      <th>Reach</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @media.each do |medium| %>
      <tr>
        <td><%= medium.title %></td>
        <td><%= medium.description %></td>
        <td><%= medium.media_type %></td>
        <td><%= medium.market_id %></td>
        <td><%= medium.media_target %></td>
        <td><%= medium.media_reach %></td>
        <td><%= medium.media_url %></td>
        <td><%= link_to 'Show', medium %></td>
        <td><%= link_to 'Edit', edit_medium_path(medium) %></td>
        <td><%= link_to 'Destroy', medium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
<br>

<%= link_to 'New Medium', new_medium_path %>
